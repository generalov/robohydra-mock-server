    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "6.0.45",
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "ts-loader": "0.9.5",
    "ts-node": "^1.4.3",
    "tslint": "3.15.1",
